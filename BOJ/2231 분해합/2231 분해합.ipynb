    "num = int(input())\n",
    "\n",
    "result = 0\n",
    "result = list()\n",
    "for i in range(1, num):\n",
    "    temp = i  \n",
    "    for j in str(i):\n",
    "        temp += int(j)\n",
    "    if num == temp:\n",
    "        result.append(i)\n",
    "\n",
    "if len(result) != 0:\n",
    "    final = min(result)\n",
    "else:\n",
    "    final = 0\n",
    "            \n",
    "print(final)"
